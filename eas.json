{
	"cli": {
		"version": ">= 16.3.2",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "dev"
			}
		},
		"preview": {
			"distribution": "internal"
		},
		"production": {
			"autoIncrement": true,
			"env": {
				"APP_VARIANT": "prod"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
