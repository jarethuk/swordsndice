{
	"name": "swordsndice",
	"version": "1.0.0",
	"scripts": {
		"android": "expo run:android",
		"ios": "expo run:ios",
		"start": "expo start --clear",
		"prebuild": "expo prebuild",
		"check": "npx tsc",
		"format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && biome check --write .",
		"web": "expo start --web",
		"profiles": "ts-node ./scripts/ProfileEnter.ts",
		"generate": "ts-node ./scripts/Generate.ts",
		"db:generate": "npx drizzle-kit generate",
		"build:web": "pnpm check && expo export -p web",
		"build:ios": "eas build --platform ios --non-interactive",
		"build:android": "eas build --platform android ",
		"build:dev": "eas build:ios --platform ios --profile development --non-interactive",
		"submit": "eas submit --platform ios",
		"submit:android": "eas submit --platform android",
		"ship": "pnpm build:ios && pnpm submit",
		"ship:android": "pnpm build:android && pnpm submit",
		"ship:dev": "pnpm build:dev"
	},
	"dependencies": {
		"@awesome.me/kit-34e2017de2": "^1.0.4",
		"@expo-google-fonts/nunito": "^0.4.2",
		"@expo/metro-runtime": "~6.1.2",
		"@fortawesome/fontawesome-svg-core": "^7.1.0",
		"@fortawesome/react-native-fontawesome": "^0.3.2",
		"@gorhom/bottom-sheet": "^5.2.7",
		"@react-native-async-storage/async-storage": "2.2.0",
		"@react-navigation/bottom-tabs": "^7.9.0",
		"@tailwindcss/postcss": "^4.1.13",
		"@tanstack/react-query": "^5.90.11",
		"@tanstack/react-query-persist-client": "^5.90.13",
		"axios": "^1.13.2",
		"babel-plugin-inline-import": "^3.0.0",
		"babel-preset-expo": "~54.0.9",
		"bad-words": "3.0.4",
		"bignumber.js": "^9.3.1",
		"clsx": "^2.1.1",
		"dayjs": "^1.11.19",
		"expo": "^54.0.27",
		"expo-camera": "~17.0.10",
		"expo-clipboard": "~8.0.8",
		"expo-constants": "~18.0.11",
		"expo-dev-client": "~6.0.20",
		"expo-font": "~14.0.10",
		"expo-image": "~3.0.11",
		"expo-image-picker": "~17.0.9",
		"expo-linking": "~8.0.10",
		"expo-router": "~6.0.17",
		"expo-splash-screen": "~31.0.12",
		"expo-status-bar": "~3.0.9",
		"nativewind": "preview",
		"postcss": "^8.5.6",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-native": "0.81.5",
		"react-native-css": "latest",
		"react-native-gesture-handler": "~2.28.0",
		"react-native-reanimated": "~4.1.1",
		"react-native-safe-area-context": "~5.6.2",
		"react-native-screens": "~4.16.0",
		"react-native-svg": "15.12.1",
		"react-native-toast-message": "^2.3.3",
		"react-native-web": "^0.21.2",
		"react-native-worklets": "0.5.1",
		"zustand": "^5.0.9"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
